{
  "name": "promboards",
  "version": "0.0.1",
  "description": "some prometheus boards",
  "author": "Mark Nevill",
  "main": "''",
  "scripts": {
    "dev": "webpack -d --watch --progress",
    "build": "webpack --config webpack.config.js --progress --profile --colors",
    "lint": "eslint --ext .js,.jsx src",
    "generate:schema": "protobuf-jsonschema src/config.proto > src/config.jsonschema && sed -i '2 i \\  \"$ref\": \"#/definitions/Config\",' src/config.jsonschema",
    "lint:config": "z-schema --noTypeless --forceItems --assumeAdditional src/config.jsonschema config.json"
  },
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.11.6",
    "eslint": "^3.4.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-react": "^6.2.0",
    "jsonschema-protobuf": "^1.0.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tap": "^3.0.1",
    "karma-webpack": "^1.8.0",
    "protobuf-jsonschema": "^1.0.0",
    "tape": "^4.6.0",
    "webpack": "^1.13.2",
    "z-schema": "^3.17.0"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "jquery": "^3.1.0",
    "moment": "^2.14.1",
    "plottable": "^2.2.0",
    "querystring": "^0.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "dlmr/react-router-redux#399fa61f40",
    "redux": "^3.6.0",
    "svg-typewriter": "^0.3.0-beta.0",
    "underscore": "^1.8.3"
  }
}
